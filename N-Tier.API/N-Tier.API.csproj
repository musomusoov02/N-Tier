    <RootNamespace>N_Tier.API</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
	  <Content Include="Properties\launchSettings.json" />
  </ItemGroup>

  <ItemGroup>
	  <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.10" />
