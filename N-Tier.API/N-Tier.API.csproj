	  <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="6.0.20" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\N-Tier.Application\N-Tier.Application.csproj" />
